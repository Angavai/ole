            <xml-property name="queryText"><![CDATA[select
ODBI.TITLE as 'Title',
IF(ODI.CALL_NUMBER IS NULL OR ODI.CALL_NUMBER = '', IF(ODH.CALL_NUMBER IS NULL,'',ODH.CALL_NUMBER), ODI.CALL_NUMBER) as 'Call Number',
ODI.BARCODE as 'Barcode',
IF(ODI.LOCATION IS NULL OR ODI.LOCATION = '', IF(ODH.LOCATION IS NULL,'',ODH.LOCATION ), ODI.LOCATION) as 'Curent Location',
OCIT.ITM_TYP_NM as 'Item Type',
ODIAS.ITEM_AVAIL_STAT_NM as 'Item Status',
ODI.DATE_CREATED as 'Date Created',
(select MAX(CHECK_IN_DT_TIME) from OLE_DLVR_TEMP_CIRC_RECORD as ODTCR
where ODTCR.ITM_ID = ODI.BARCODE) as 'Date Last Loaned'
from 
OLE_DLVR_ITEM_AVAIL_STAT_T as ODIAS,
OLE_DS_ITEM_T as ODI,
OLE_DS_HOLDINGS_T as ODH,
OLE_DS_BIB_INFO_T as ODBI,
OLE_CAT_ITM_TYP_T as OCIT
where
ODIAS.ITEM_AVAIL_STAT_CD = 'LOANED'
and 
ODIAS.ITEM_AVAIL_STAT_ID = ODI.ITEM_STATUS_ID
and
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and
ODI.ITEM_TYPE_ID = OCIT.ITM_TYP_CD_ID;
]]></xml-property>
