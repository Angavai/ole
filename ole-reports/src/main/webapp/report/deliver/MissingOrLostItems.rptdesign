            <xml-property name="queryText"><![CDATA[select 
ODBI.TITLE as 'Title',
ODBI.AUTHOR as 'Author',
IF(ODI.CALL_NUMBER IS NULL OR ODI.CALL_NUMBER = '', IF(ODH.CALL_NUMBER IS NULL,'',ODH.CALL_NUMBER), ODI.CALL_NUMBER) as 'Call Number',
ODI.ENUMERATION as 'Enumaeration',
IF(ODI.LOCATION IS NULL OR ODI.LOCATION = '', IF(ODH.LOCATION IS NULL,'',ODH.LOCATION ), ODI.LOCATION) as 'Location',
OCIT.ITM_TYP_NM as 'Item Type',
IF(ODI.MISSING_PIECES = 'Y',ODI.MISSING_PIECES_EFFECTIVE_DATE,ODI.ITEM_STATUS_DATE_UPDATED)as 'Date Missing/Lost Status Added'
from 
OLE_DS_ITEM_T as ODI,
OLE_DS_HOLDINGS_T as ODH,
OLE_DS_BIB_INFO_T as ODBI,
OLE_CAT_ITM_TYP_T as OCIT,
OLE_DLVR_ITEM_AVAIL_STAT_T as ODIAS
where
ODIAS.ITEM_AVAIL_STAT_ID = ODI.ITEM_STATUS_ID
and
(ODIAS.ITEM_AVAIL_STAT_CD = 'LOST' or ODI.MISSING_PIECES = 'Y')
and 
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and
ODI.ITEM_TYPE_ID = OCIT.ITM_TYP_CD_ID]]></xml-property>
