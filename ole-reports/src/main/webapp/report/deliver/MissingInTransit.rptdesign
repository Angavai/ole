            <xml-property name="queryText"><![CDATA[select 
OLIH.CIR_DESK_LOC as "Source Location",
OLIH.CIR_DESK_ROUTE_TO as "Target Location",
ODI.ITEM_ID as "Item Id",
CASE WHEN ODI.CALL_NUMBER IS NOT NULL AND ODI.CALL_NUMBER != ' ' THEN ODI.CALL_NUMBER 
	WHEN ODH.CALL_NUMBER IS NOT NULL AND ODH.CALL_NUMBER != ' ' THEN ODH.CALL_NUMBER 
	ELSE ' ' 
END as "Call Number",
ODBI.TITLE as "Title",
OLIH.ITEM_RETURNED_DT as "In Transit Date"
from 
OLE_DS_ITEM_T ODI,
OLE_DLVR_ITEM_AVAIL_STAT_T ODIAS,
OLE_DS_HOLDINGS_T ODH,
OLE_DS_BIB_INFO_T ODBI,
OLE_LOAN_INTRANSIT_HISTORY_T OLIH
where 
ODI.MISSING_PIECES = 'Y'
and
ODI.ITEM_STATUS_ID = ODIAS.ITEM_AVAIL_STAT_ID
and
ODIAS.ITEM_AVAIL_STAT_CD = 'INTRANSIT'
and 
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and
OLIH.ITEM_BARCODE = ODI.BARCODE
and
OLIH.ITEM_RETURNED_DT IN (select MAX(ITEM_RETURNED_DT) from
OLE_LOAN_INTRANSIT_HISTORY_T OLIH
group by OLIH.ITEM_BARCODE)]]></xml-property>
