            <xml-property name="queryText"><![CDATA[select 
concat(OPEV.FIRST_NM,' ',OPEV.LAST_NM) as 'Patron Name',
OPEV.PTRN_TYP as 'Patron Type',
OPEV.EMAIL as 'Email Address',
OPEV.PHONE as 'Phone Number',
IF(ODI.LOCATION IS NULL OR ODI.LOCATION = '', IF(ODH.LOCATION IS NULL,'',ODH.LOCATION ), ODI.LOCATION) as 'Item Location',
OCIT.ITM_TYP_NM as 'Item Type',
ODBI.TITLE as 'Title',
ODBI.AUTHOR as 'Author',
ODL.CURR_DUE_DT_TIME as 'Due Date'
from 
OLE_DLVR_LOAN_T as ODL,
OLE_PTRN_ENTITY_V as OPEV,
OLE_DS_ITEM_T as ODI,
OLE_DS_HOLDINGS_T as ODH,
OLE_DS_BIB_INFO_T as ODBI,
OLE_CAT_ITM_TYP_T as OCIT
where 
DATEDIFF(CURDATE(), ODL.CURR_DUE_DT_TIME) > 0
and
ODL.OLE_PTRN_ID = OPEV.OLE_PTRN_ID
and
ODL.ITM_ID = ODI.BARCODE
and 
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and
ODI.ITEM_TYPE_ID = OCIT.ITM_TYP_CD_ID;]]></xml-property>
