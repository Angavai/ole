            <xml-property name="queryText"><![CDATA[select 
OLIH.CIR_DESK_ROUTE_TO as 'Destination',
OLIH.ITEM_RETURNED_DT as 'In Transit Date',
OLIH.CIR_DESK_LOC as 'Circulation Desk Where Returned',
IF(ODI.CALL_NUMBER IS NULL OR ODI.CALL_NUMBER = '', IF(ODH.CALL_NUMBER IS NULL,'',ODH.CALL_NUMBER), ODI.CALL_NUMBER) as 'Call Number',
ODBI.TITLE as 'Title',
concat(OPEV.FIRST_NM,' ',OPEV.LAST_NM) as 'Patron Name',
ODRT.OLE_RQST_TYP_NM as 'Request Type'
from
OLE_DLVR_RQST_T as ODR,
OLE_DS_ITEM_T as ODI,
OLE_DS_HOLDINGS_T as ODH,
OLE_DLVR_ITEM_AVAIL_STAT_T as ODIAS,
OLE_PTRN_ENTITY_V as OPEV,
OLE_DS_BIB_INFO_T as ODBI,
OLE_LOAN_INTRANSIT_HISTORY_T as OLIH,
OLE_DLVR_RQST_TYP_T as ODRT
where
(ODR.OLE_RQST_TYP_ID = 2 or ODR.OLE_RQST_TYP_ID = 4 or ODR.OLE_RQST_TYP_ID = 6)
and
ODR.ITM_ID = ODI.BARCODE
and
ODI.ITEM_STATUS_ID = ODIAS.ITEM_AVAIL_STAT_ID
and
ODIAS.ITEM_AVAIL_STAT_CD = 'INTRANSIT-FOR-HOLD'
and 
OLIH.ITEM_BARCODE = ODI.BARCODE
and
ODR.OLE_PTRN_ID = OPEV.OLE_PTRN_ID
and 
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and 
ODRT.OLE_RQST_TYP_ID = ODR.OLE_RQST_TYP_ID
and
OLIH.ITEM_RETURNED_DT IN (select MAX(ITEM_RETURNED_DT) from
OLE_LOAN_INTRANSIT_HISTORY_T as OLIH
group by OLIH.ITEM_BARCODE);]]></xml-property>
